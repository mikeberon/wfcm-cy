  "bugs": {
    "url": "https://github.com/kjt01/test-repo/issues"
  },
  "homepage": "https://github.com/kjt01/test-repo#readme",
  "dependencies": {
    "@bahmutov/cy-api": "^2.2.6",
    "ini": "^4.1.2"
  },
  "devDependencies": {
    "cypress": "^13.7.2",
    "cypress-html-reporter": "^11.0.0",
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0"
  }
}
