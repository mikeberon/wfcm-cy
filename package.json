{
  "name": "wfcm-cy",
  "version": "1.0.0",
  "description": "wfcm scripts using cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:record": "npx cypress run --record --browser chrome --key 726eb451-3245-4f64-bb8d-d0263ab72517 --variable CYPRESS_USERNAME",
    "cy:parallel": "npx cypress run --browser chrome --parallel --record --key 726eb451-3245-4f64-bb8d-d0263ab72517 --ci-build-id $(date +%s)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjt01/test-repo.git"
  },
  "author": "Mike Beron",
